#include<bits/stdc++.h>
using namespace std;

int main()
{
    string a;
    cin>>a;
    for(int i=0;i<a.length();i++)
    {
        if(a[i]=='1' && a[i+1]=='4' && a[i+2]=='4')
        {
            i=i+2;
        }
        else if(a[i]=='1' && a[i+1]=='4')
        {
            i=i+1;
        }
        else if(a[i]=='1')
            continue;
        else
        {
            cout<<"NO"<<endl;
            return 0;
        }
    }
    cout<<"YES"<<endl;
    return 0;
}
