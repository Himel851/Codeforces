#include<iostream>
using namespace std;

int main()
{
    int x,a,b,c,d;
    cin>>x;
    while(x++)
     {
        d=x%10;
        c=(x/10)%10;
        b=(x/100)%10;
        a=(x/1000)%10;

        if(a!=b && a!=c && a!=d && b!=c && b!=d && c!=d)
        {
            cout<<x<<endl;
            return 0;
        }

     }
}
