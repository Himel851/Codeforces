#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a,l;
    string x;
    cin>>a;
    while(a--)
    {
        cin>>x;
        l=x.length();
        if((x[l-2]=='c' && x[l-1]=='h') || x[l-1]=='x' ||  x[l-1]=='s' || x[l-1]=='o')
            cout<<x<<"es"<<endl;
        else if(x[l-1]=='f')
        {
            x[l-1]='v';
            cout<<x<<"es"<<endl;
        }
        else if(x[l-2]=='f' && x[l-1]=='e')
        {
            x[l-2]='v';
            cout<<x<<'s'<<endl;
        }

        else if(x[l-1]=='y')
        {
            x[l-1]='i';
            cout<<x<<"es"<<endl;
        }
        else
            cout<<x<<'s'<<endl;

    }
    return 0;
}
