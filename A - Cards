#include<bits/stdc++.h>
using namespace std;
int main()
{

    int x,n=0,z=0;
    string a;
    cin>>x>>a;
    sort(a.begin(),a.end());
    for(int i=0;i<a.length();i++)
    {
        if(a[i]=='n')
            n++;
        else if(a[i]=='z')
            z++;
    }
    for(int i=1;i<=n;i++)
        cout<<"1 ";
    for(int i=1;i<=z;i++)
          {
            cout<<"0 ";
          }
        cout<<endl;


    return 0;
}
