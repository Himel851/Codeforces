#include<iostream>
#include<algorithm>
#include<stdio.h>

using namespace std;

int main()

{
    string a,b;
    cin>>a>>b;
    int i,j,z;
    z=a.length();

    for(i=0;i<z;i++)
    {
        a[i]=tolower(a[i]);
        b[i]=tolower(b[i]);
    }


        if(a==b)
            cout<<"0"<<endl;

        else if(a<b)
            cout<<"-1"<<endl;
        else if(a>b)
            cout<<"1"<<endl;


    return 0;
}

