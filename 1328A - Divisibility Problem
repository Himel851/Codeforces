#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,a,b,p,ans;
    cin>>t;
    while(t--)
    {
        cin>>a>>b;

        if(a%b==0)
        {
            cout<<'0'<<endl;
        }
        else{
                p=(a/b)+1;
                ans=(b*p)-a;
                cout<<ans<<endl;


        }

    }
    return 0;
}


