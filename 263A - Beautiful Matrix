#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,total,a[10][10];
    for(int i=1;i<=5;i++)
    {
        for(int j=1;j<=5;j++)
        {
            cin>>a[i][j];
            if(a[i][j]==1)
            {
                total=abs(3-i)+abs(3-j);
            }
        }
    }
    cout<<total<<endl;
    return 0;
}
