#include<iostream>
using namespace std;
int main()
{
    string a,b;
    b="hello";
    int j=0;
    int count=0;
    cin>>a;
    for(int i=0;i<a.length();i++)
    {
        if(a[i]==b[j])
        {
            j++;
            count++;
        }
    }
    if(count==5)
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
}
